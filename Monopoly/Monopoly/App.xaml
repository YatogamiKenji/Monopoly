<Application x:Class="Monopoly.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Monopoly"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/BtnStyle1.xaml" />
                <ResourceDictionary Source="/Styles/CardStyle.xaml" />
                <ResourceDictionary Source="/Styles/NotiStyle.xaml" />
                <ResourceDictionary Source="/Styles/ScrollBarStyle.xaml" />
                <ResourceDictionary Source="/Layouts/PanelCenterMap.xaml"/>
                <ResourceDictionary Source="/Layouts/NotiBoxFrame.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="mainFont">/Monopoly;component/font/HP-Yard Regular.ttf#HP-Yard</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
